{
    "manifest_version": 3,
    "name": "DeskTube",
    "version": "1.0.0",
    "description": "DeskTube",
    "icons": {
    	"16":"assets/16.png",
    	"32":"assets/32.png",
    	"128":"assets/128.png",
    	"144":"assets/144.png",
       	"192":"assets/192.png",
    	"512":"assets/512.png"
    },
    "author": "malisipi",
    "action": {
        "default_popup": "index.html",
        "default_title": "DeskTube"
    },
    "side_panel": {
        "default_path": "index.html"
    },
    "sidebar_action": {
        "default_icon": "assets/144.png",
        "default_title": "DeskTube",
        "default_panel": "index.html"
    },
    "permissions": [
        "sidePanel"
    ]
}
